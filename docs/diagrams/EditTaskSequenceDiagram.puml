@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant "u:EditTaskCommand" as EditTaskCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box
[-> LogicManager : execute(editTask)
activate LogicManager

LogicManager -> AddressBookParser : parseCommand(editTask))
activate AddressBookParser

create EditTaskCommand
AddressBookParser -> EditTaskCommand
activate EditTaskCommand

EditTaskCommand --> AddressBookParser
deactivate EditTaskCommand

AddressBookParser --> LogicManager : u
deactivate AddressBookParser

LogicManager -> EditTaskCommand : execute()
activate EditTaskCommand

EditTaskCommand -> Model : setTask()
deactivate Model

EditTaskCommand --> LogicManager : result
deactivate EditTaskCommand
EditTaskCommand -[hidden]-> LogicManager : result
destroy EditTaskCommand

[<--LogicManager
deactivate LogicManager
@enduml
